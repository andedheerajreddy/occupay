<body>

    <div class="">

        <!-- Sign up form -->
        <section class="signup">
            <div class="container">

                <div style="text-align: center; margin-left: 25%;margin-right: 10%; width: 50%;">
                    <h2 class="form-title" style="text-align: center;padding-top: 10% !important">Sign up</h2>
                    <div class="form-group">
                        <label for="name"><i class="zmdi zmdi-account material-icons-name"></i></label>
                        <input type="text" name="name" id="name" placeholder="Your Name" />
                    </div>
                    <div class="form-group">
                        <label for="email"><i class="zmdi zmdi-email"></i></label>
                        <input type="email" name="email" id="email" placeholder="Your Email" />
                    </div>
                    <div class="form-group">
                        <label for="pass"><i class="zmdi zmdi-lock"></i></label>
                        <input type="password" name="pass" id="pass" placeholder="Password" />
                    </div>


                    <div class="form-group">
                        <label for="pass"><i class="fas fa-calendar-day"></i></label>
                        <input placeholder="Enter DOB" class="textbox-n" type="text" onfocus="(this.type='date')" id="date">
                    </div>

                    <div class="form-group">
                        <label for="name"><i class="fas fa-road"></i></label>
                        <input type="text" name="name" id="name" placeholder="Street" />

                    </div>

                    <div class="form-group">
                        <label for="name"><i class="fas fa-building"></i></label>
                        <select name="gfg" id="" aria-placeholder="select state">
                            <option value="" hidden>select state</option>
                                            <option value="dsfds">dfdsaf</option>
                            <option value="cdfdf">fsgf</option>
                        </select> </div>
                    <div class="form-group">
                        <label for="name"><i class="fa fa-map-pin" aria-hidden="true"></i>
                        </label>
                        <input type="number" name="name" id="name" placeholder="Pincode" />
                    </div>
                    <div class="form-group">
                        <label for="name"><i class="fas fa-city"></i></label>
                        <select name="gfg" id="" aria-placeholder="select state">
                            <option value="" hidden>select state</option>
                                            <option value="dsfds">dfdsaf</option>
                            <option value="cdfdf">fsgf</option>
                        </select> </div>
                    <div class="form-group">
                        <label for="name"><i class="fas fa-globe-asia"></i></label>
                        <select name="gfg" id="" aria-placeholder="select state">
                            <option value="" hidden>select state</option>
                                            <option value="dsfds">dfdsaf</option>
                            <option value="cdfdf">fsgf</option>
                        </select> </div>
                    <div class="form-group">
                        <input type="checkbox" name="agree-term" id="agree-term" class="agree-term" />

                        <label for="agree-term" class="label-agree-term"><span><span></span></span>I agree all statements in  <a href="#" class="term-service">Terms of service</a></label>
                    </div>
                    <div class="form-group form-button" style="padding-bottom: 10%;">
                        <input type="button" name="signup" id="signup" class="form-submit" value="Register" />
                    </div>

                </div>


            </div>
        </section>

        <!-- Sing in  Form -->

    </div>

    <!-- JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
